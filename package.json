  },
  "devDependencies": {
    "depcheck": "^0.8.0",
    "form-data": "^2.3.3",
    "husky": "^1.3.1",
    "istanbul": "^0.4.5",
    "licensee": "^7.0.1",
    "lint-staged": "^8.1.7",
    "reformat-json-files": "^1.0.1",
    "standard": "^12.0.1",
    "tape": "^4.10.1"
  },
  "scripts": {
    "coverage": "NODE_ENV=test istanbul cover -- tape test/*.test.js",
    "postcoverage": "istanbul check-coverage coverage/coverage.json",
    "deps": "depcheck --ignores standard,licensee,husky,lint-staged,reformat-json-files",
    "lint": "standard",
    "licenses": "licensee --errors-only",
    "test": "NODE_ENV=test tape test/*.test.js"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ],
    "*.json": [
      "reformat-json-files",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
